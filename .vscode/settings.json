{
  "editor.tabSize": 2,
  "editor.defaultFormatter": "vscode.typescript-language-features",
  "editor.formatOnSave": true,
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": [
    "broadcast-channel",
    "cron",
    "kv",
    "webgpu"
  ],
  "[markdown]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
}